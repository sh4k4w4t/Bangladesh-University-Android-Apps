<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragmentItems.Admission.Admission">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="10dp">

        <TextView
            android:id="@+id/admission_start"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="7dp"
            android:gravity="center_horizontal"
            android:padding="3dp"
            android:textAllCaps="true"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@color/color_code_main_1"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/student_enrollment_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="7dp"
            android:background="@color/color_code_main_3"
            android:gravity="center_horizontal"
            android:padding="3dp"
            android:textAllCaps="true"
            android:textColor="@color/color_code_main_1"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/student_enrollment_title_one"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="@string/app_name"
            tools:ignore="UnusedAttribute" />

        <TextView
            android:id="@+id/student_enrollment_title_two"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:justificationMode="inter_word"
            android:text="@string/app_name"
            tools:ignore="UnusedAttribute" />

        <TextView
            android:id="@+id/under_graduation_admission_one"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="7dp"
            android:background="@color/color_code_main_3"
            android:gravity="center_horizontal"
            android:padding="3dp"
            android:textAllCaps="true"
            android:textColor="@color/color_code_main_1"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/under_graduation_admission_two"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="@string/app_name"
            tools:ignore="UnusedAttribute" />

        <TextView
            android:id="@+id/under_graduation_admission_three"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:justificationMode="inter_word"
            android:text="@string/app_name"
            tools:ignore="UnusedAttribute" />

        <TextView
            android:id="@+id/graduation_admission_one"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="7dp"
            android:background="@color/color_code_main_3"
            android:gravity="center_horizontal"
            android:padding="3dp"
            android:textAllCaps="true"
            android:textColor="@color/color_code_main_1"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/graduation_admission_two"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="@string/app_name"
            tools:ignore="UnusedAttribute" />

        <TextView
            android:id="@+id/graduation_admission_three"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="@string/app_name"
            tools:ignore="UnusedAttribute" />

        <TextView
            android:id="@+id/fa_one"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="7dp"
            android:background="@color/color_code_main_3"
            android:gravity="center_horizontal"
            android:padding="3dp"
            android:textAllCaps="true"
            android:textColor="@color/color_code_main_1"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fa_two"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="@string/app_name"
            tools:ignore="UnusedAttribute" />

        <TextView
            android:id="@+id/fa_three"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="@string/app_name"
            tools:ignore="UnusedAttribute" />

        <TextView
            android:id="@+id/fa_four"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="@string/app_name"
            tools:ignore="UnusedAttribute" />
    </LinearLayout>

</androidx.core.widget.NestedScrollView>