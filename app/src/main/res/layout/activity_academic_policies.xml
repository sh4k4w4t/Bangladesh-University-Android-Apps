            android:background="@color/color_code_main_3"/>

        <TextView
            android:id="@+id/improvement_Examination_two"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/app_name"
            android:justificationMode="inter_word"
            tools:ignore="UnusedAttribute"
            android:layout_marginBottom="10dp"/>

        <TextView
            android:id="@+id/dropout_Course_due_to_disciplinary_action_Recourse_Examinations_one"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textAllCaps="true"
            android:textColor="@color/color_code_main_1"
            android:gravity="center_horizontal"
            android:padding="3dp"
            android:layout_marginBottom="7dp"
            android:layout_marginTop="10dp"
            android:background="@color/color_code_main_3"/>

        <TextView
            android:id="@+id/dropout_Course_due_to_disciplinary_action_Recourse_Examinations_two"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/app_name"
            android:justificationMode="inter_word"
            tools:ignore="UnusedAttribute"
            android:layout_marginBottom="10dp"/>
    </LinearLayout>

</androidx.core.widget.NestedScrollView>